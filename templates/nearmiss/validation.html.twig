<html lang="en">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Validation</title>
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
	</head>
	<body>
		<div class="container mb-5">
			<div class="row">
				<div class="col-12 col-md-11 col-lg-7 col-xl-6 mx-auto">
					<div class="app-branding text-center mb-5">
						<img src="{{ asset('img/I_own_safety.png') }}" width="200" height="90" alt="logo">
					</div>
					<!--//app-branding-->
					<div class="app-card p-5 text-center shadow-sm">
						<h1 class="page-title mb-4">Near miss validated<br><span class="font-weight-light"></span>
						</h1>
						<div class="mb-4">
							Cet near miss a été validé par la direction
						</div>
						<a class="btn app-btn-primary" href="{{ path('app_home') }}">Go to home page</a>
					</div>
				</div>
				<!--//col-->
			</div>
			<!--//row-->
		</div>
		<!--//container-->
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</body>
</html>
